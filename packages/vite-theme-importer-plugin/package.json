{
  "name": "@repo/vite-theme-importer-plugin",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "postinstall": "tsup src/index.ts --clean --dts --format cjs,esm",
    "watch": "npm run postinstall -- --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.2"
  }
}
