{
  "name": "@repo/vite-theme-plugin",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "private": true,
  "scripts": {
    "postinstall": "tsup src/index.ts --clean --dts --format cjs,esm",
    "watch": "npm run postinstall -- --watch",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.2"
  }
}
