{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["VITE_THEME"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "serve": {
      "dependsOn": ["start"]
    },
    "lint": {
      "outputs": []
    },
    "type-check": {},
    "start": {
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
